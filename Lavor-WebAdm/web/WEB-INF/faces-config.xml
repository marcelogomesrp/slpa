    <managed-bean>
        <managed-bean-name>tt</managed-bean-name>
        <managed-bean-class>lavor.backbean.templateclient</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    -->
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/admin/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addusuario</from-outcome>
            <to-view-id>/admin/usuario/adicionar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>categoria</from-outcome>
            <to-view-id>/admin/categoria/adicionar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>









